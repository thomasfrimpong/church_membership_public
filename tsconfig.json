{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts" ,"app/api/members/[id]/route.js", "app/api/admins/route.js", "app/users/components/BasicTable.jsx", "app/dashboard/page.jsx", "app/api/users/route.js", "app/api/summaries/route.js", "app/api/members/route.js", "app/api/change_password/route.js", "app/admins/components/BasicTable.jsx", "app/admins/create/page.jsx", "components/NavBar.jsx", "components/AppBarComponent.tsx", "app/api/auth/[...nextauth]/route.js", "app/layout.jsx"],
  "exclude": ["node_modules"]
}
